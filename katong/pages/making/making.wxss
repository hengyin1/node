page {
    background-color: #f2f2f2;
}

.container {
    width: 100%;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}

.make-result-img {
    display: block;
    width: 100%;
    height: 60vh;
    margin-top: 34rpx;
}

.nav {
    height: 96rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 96rpx;
    font-size: 16px;
    white-space: nowrap;
    left: 0;
    z-index: 99;
}

.nav-list {
    width: 100%;
    height: 96rpx;
}

.nav-item {
    position: relative;
    width: 19.666%;
    overflow: hidden;
    display: inline-block;
    text-align: center;
    font-size: 28rpx;
    color: #888;
}

.nav-item.active {
    color: #000;
    transition: transform .5s;
    transition: transform .5s,-webkit-transform .5s;
    transition: transform .5s,-webkit-transform .5s;
}

.make-img-list {
    width: 100%;
    height: 150rpx;
    white-space: nowrap;
}

.scroll-list {
    width: 100%;
    height: 150rpx;
}

.sub-imglist .sub-cur {
    width: 140rpx;
    height: 150rpx;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.6);
    border-radius: 4px;
    text-align: center;
}

.sub-imglist .sub-cur .img {
    width: 30rpx;
    height: 30rpx;
}

.make-img-list .sub-imglist {
    width: 140.5rpx;
    height: 150rpx;
    position: relative;
    margin-left: 20rpx;
    display: inline-block;
    border-radius: 8rpx;
}

.sub-imglist .vip-img {
    width: 140rpx;
    height: 150rpx;
    overflow: hidden;
    display: inline-block;
}

.sub-imglist:last-child {
    margin-right: 20rpx;
}

.sub-imglist .img {
    width: 140rpx;
    border-radius: 8rpx 8rpx 0 0;
}

.pr {
    position: relative;
}

.pa {
    position: absolute;
}

image,.img {
    width: 100%;
}

.fl {
    float: left;
}

.flex-row-center {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.button {
    width: 330rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
}

button::after {
    border: none;
}

.ch-button {
    border: 1px solid #dbdbdb;
    background-color: transparent;
    /* background-color: #ff9966; */
}




